            IfStmt(
                c=BoolEq(Var(value):untyped, Var(VALUE_ONE):untyped):untyped, 
                left=
                    Return(LitExpr(int, 1):untyped);
            );
            IfStmt(
                c=BoolEq(Var(value):untyped, Var(VALUE_TWO):untyped):untyped, 
                left=
                    Return(LitExpr(int, 2):untyped);
