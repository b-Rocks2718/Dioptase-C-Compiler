                    CaseStmt(
                        Var(VALUE_ONE):untyped,
                        BreakStmt(label=null);
                    );
                    CaseStmt(
                        Var(VALUE_TWO):untyped,
                        BreakStmt(label=null);
                    );
                    CaseStmt(
                        Var(VALUE_THREE):untyped,
                        BreakStmt(label=null);
                    );
                    CaseStmt(
                        Var(VALUE_FOUR):untyped,
                        BreakStmt(label=null);
                    );
                );
